<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bean">
 <class name="Subject">
  <id column="id" name="id" type="java.lang.Integer">
   <generator class="assigned"/>
  </id>
  <property column="SubjectName" generated="never" lazy="false"
   name="SubjectName" not-null="true" type="java.lang.String" unique="true"/>
  <property column="SubjectPoint" generated="never" lazy="false"
   name="SubjectPoint" not-null="true" type="java.lang.Double"/>
 </class>
</hibernate-mapping>

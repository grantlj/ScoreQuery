<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bean">
 <class name="Admin">
  <id column="id" name="id" type="java.lang.Integer">
   <generator class="increment"/>
  </id>
  <property column="username" generated="never" lazy="false"
   name="username" not-null="true" type="java.lang.String" unique="true"/>
  <property column="userpwd" generated="never" lazy="false"
   name="userpwd" not-null="true" type="java.lang.String"/>
 </class>
</hibernate-mapping>
